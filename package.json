    "@types/react-dom": "^16.8.4",
    "enzyme": "^3.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts-ts": "^3.1.0"
  }
